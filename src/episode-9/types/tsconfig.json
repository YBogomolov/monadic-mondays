{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": false,
    "noImplicitThis": true,
    "alwaysStrict": true,
    // "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "lib": [
      "es5"
    ],
    "baseUrl": ".",
    "paths": {
      "typelevel": [
        "."
      ]
    }
  }
}